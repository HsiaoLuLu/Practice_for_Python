#九九乘法表
for x in range(1,10):
    for y in range(1, 10):
        print('%d * %d = %d' % (x, y, x*y),end = '\t')
    print()



#最大公因數,最小公倍數
x = int(input('x:'))
y = int(input('y:'))
z = 0
for i in range(1, y+1):
    if x % i == 0 and y % i == 0:
        z = i

print('x, y 最大公因數為：%d' % z)
print('x, y 最小公倍數為：%d' % (x * y / z ))
